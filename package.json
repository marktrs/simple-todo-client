{
	"name": "simple-todo-client",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vite build && vitest run",
		"test:ui": "vite build && vitest --ui",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . --config .prettierrc && eslint .",
		"format": "prettier --plugin-search-dir . --config .prettierrc --write .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/preset-env": "^7.21.4",
		"@playwright/test": "^1.32.3",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/adapter-node": "^1.2.3",
		"@sveltejs/kit": "^1.15.8",
		"@sveltejs/vite-plugin-svelte": "^2.1.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@types/jest": "^29.5.1",
		"@types/mocha": "^10.0.1",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"@vitest/browser": "^0.30.1",
		"@vitest/ui": "^0.30.1",
		"autoprefixer": "^10.4.14",
		"babel-jest": "^29.5.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"jsdom": "^21.1.1",
		"playwright": "^1.32.3",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.3.2",
		"ts-jest": "^29.1.0",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.2",
		"vitest": "^0.30.1",
		"webdriverio": "^8.8.6"
	},
	"type": "module",
	"dependencies": {
		"@jridgewell/sourcemap-codec": "^1.4.15",
		"@popperjs/core": "^2.11.7",
		"classnames": "^2.3.2",
		"dayjs": "^1.11.7",
		"flowbite": "^1.6.5",
		"flowbite-svelte": "^0.34.9"
	}
}
